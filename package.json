{
  "name": "jov2",
  "version": "0.4.2",
  "description": "A light Javascript API for reading OV2 POI databases",
  "main": "index.js",
  "scripts": {
    "test": "nyc --reporter=lcov mocha ./test/main.js",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "display-coverage": "opener ./coverage/lcov-report/index.html",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "report": "nyc report",
    "doc": "vuepress dev docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jclo/jov2.git"
  },
  "keywords": [
    "ov2",
    "POI",
    "TomTom"
  ],
  "author": {
    "name": "jclo",
    "email": "jclo@mobilabs.fr",
    "url": "http://www.mobilabs.fr/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jclo/jov2/issues"
  },
  "homepage": "https://github.com/jov2/umdlib",
  "dependencies": {
    "vuepress": "0.14.8"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "coveralls": "3.0.2",
    "eslint": "5.13.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.16.0",
    "husky": "1.3.1",
    "mocha": "5.2.0",
    "nyc": "13.2.0",
    "opener": "1.5.1"
  },
  "private": false,
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run check-coverage && npm run report"
    }
  }
}
