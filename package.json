{
  "name": "jov2",
  "version": "0.2.0",
  "description": "A light Javascript API for reading OV2 POI databases",
  "main": "index.js",
  "scripts": {
    "test": "istanbul cover -x ./test/main.js _mocha -- -R spec ./test/main.js",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "display-coverage": "opener ./coverage/lcov-report/index.html",
    "report-coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jclo/jov2.git"
  },
  "keywords": [
    "ov2",
    "POI",
    "TomTom"
  ],
  "author": {
    "name": "jclo",
    "email": "jclo@mobilabs.fr",
    "url": "http://www.mobilabs.fr/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jclo/jov2/issues"
  },
  "homepage": "https://github.com/jov2/umdlib",
  "devDependencies": {
    "chai": "3.5.0",
    "coveralls": "2.11.14",
    "eslint": "3.5.0",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-plugin-import": "1.15.0",
    "ghooks": "1.3.2",
    "istanbul": "0.4.5",
    "mocha": "3.0.2",
    "opener": "1.4.2"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test && npm run check-coverage"
    }
  }
}
